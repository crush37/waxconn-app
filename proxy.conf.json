{
  "/api/*": {
    "target": "http://localhost",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/api": "/api"
    },
    "logLevel": "debug"
  },
  "/sanctum/*": {
    "target": "http://localhost",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/sanctum": "/sanctum"
    },
    "logLevel": "debug"
  },
  "/auth/*": {
    "target": "http://localhost",
    "secure": false,
    "changeOrigin": true,
    "pathRewrite": {
      "^/auth": "/auth"
    },
    "logLevel": "debug"
  }
}
